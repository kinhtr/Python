# -*- coding: cp1252 -*-

def tester(givenstring = "Too short"):
	if len(givenstring) >= 10:
		print(givenstring)
	else:
		print("Too short")
	
def main():
	while True:
		text = input("Write something (quit ends): ")
		if text == "quit":
			break
		tester(text)
    
if __name__ == "__main__":
    main()
